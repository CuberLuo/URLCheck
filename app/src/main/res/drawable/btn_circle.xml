<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <selector>
            <item>
                <shape android:shape="oval">
                    <solid android:color="#8396AD" />
                    <size
                        android:width="100dp"
                        android:height="100dp" />
                </shape>
            </item>

        </selector>
    </item>

</layer-list>